{
  "name": "serverless-inversify-util",
  "version": "0.7.11",
  "description": "A set of decorators to be used with inversify to generate serverless config and handlers.",
  "main": "./dist/lib/index.js",
  "types": "./dist/types/lib/index.d.ts",
  "bin": {
    "serverless-inversify-util": "dist/lib/script/generate.js"
  },
  "scripts": {
    "clean": "rm -rf ./bin ./dist ./typescript/bin ./test-bin ./.nyc_output ./coverage && find ./typescript -type f -name *.js -delete",
    "prepare": "npm run clean && tsc -p ./typescript/tsconfig.json",
    "generate": "ts-node ./typescript/script/generate.ts ./example/typescript/index.ts ./example/tsconfig.json",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "test": "npm run clean && TS_NODE_PROJECT=./typescript/tsconfig.json TS_NODE_CACHE=false nyc mocha -t 100000 --recursive --compilers ts:ts-node/register ./typescript/test/lib/$FILE"
  },
  "author": "Derek Honerlaw",
  "license": "MIT",
  "nyc": {
    "exclude": [
      "**/test",
      "test-bin",
      "**/generate.ts"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "html",
      "lcov",
      "text-summary"
    ],
    "all": true
  },
  "dependencies": {
    "commander": "^2.11.0",
    "fs-extra": "^4.0.1",
    "inversify": "^4.3.0",
    "reflect-metadata": "^0.1.10",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/commander": "^2.9.2",
    "@types/fs-extra": "^4.0.0",
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.0.25",
    "@types/proxyquire": "^1.3.28",
    "@types/yamljs": "^0.2.30",
    "chai": "^4.1.1",
    "coveralls": "^2.13.1",
    "mocha": "^3.5.0",
    "nyc": "^11.1.0",
    "proxyquire": "^1.8.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.6.0",
    "typemoq": "^2.0.1",
    "typescript": "^2.4.2"
  }
}
