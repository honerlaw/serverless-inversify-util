{
  "name": "serverless-inversify-util",
  "version": "0.9.3",
  "description": "A set of decorators to be used with inversify to generate serverless config and handlers.",
  "homepage": "https://github.com/honerlaw/serverless-inversify-util",
  "repository": {
    "type": "git",
    "url": "https://github.com/honerlaw/serverless-inversify-util"
  },
  "main": "./dist/lib/index.js",
  "types": "./dist/types/lib/index.d.ts",
  "bin": {
    "serverless-inversify-util": "dist/lib/script/script.js"
  },
  "scripts": {
    "lint": "tslint ./typescript/**/*.ts",
    "clean": "rm -rf ./bin ./dist ./typescript/bin ./test-bin ./build ./.nyc_output ./coverage && find ./typescript -type f -name *.js -delete",
    "prepare": "npm run clean && tsc -p ./typescript/tsconfig.json",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "test": "npm run clean && TS_NODE_PROJECT=./typescript/tsconfig.json TS_NODE_CACHE=false nyc mocha -t 100000 --recursive --compilers ts:ts-node/register ./typescript/test/lib/$FILE"
  },
  "author": "Derek Honerlaw",
  "license": "MIT",
  "nyc": {
    "exclude": [
      "**/test",
      "test-bin",
      "**/script.ts"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "html",
      "lcov",
      "text-summary"
    ],
    "all": true
  },
  "dependencies": {
    "commander": "^2.12.1",
    "fs-extra": "^4.0.2",
    "inversify": "^4.5.2",
    "reflect-metadata": "^0.1.10",
    "webpack": "^3.8.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/chai": "^4.0.6",
    "@types/commander": "^2.11.0",
    "@types/fs-extra": "^4.0.5",
    "@types/mocha": "^2.2.44",
    "@types/node": "^8.0.53",
    "@types/proxyquire": "^1.3.28",
    "@types/webpack": "^3.8.1",
    "@types/yamljs": "^0.2.30",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "mocha": "^4.0.1",
    "nyc": "^11.3.0",
    "proxyquire": "^1.8.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "typemoq": "^2.1.0",
    "typescript": "^2.6.2"
  }
}
