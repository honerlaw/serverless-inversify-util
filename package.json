{
  "name": "serverless-inversify-util",
  "version": "0.1.2",
  "description": "A set of decorators to be used with inversify to generate serverless config and handlers.",
  "main": "./dist/lib/index.js",
  "types": "./dist/types/lib/index.d.ts",
  "bin": {
    "serverless-inversify-util": "dist/lib/script/generate.js"
  },
  "scripts": {
    "clean": "rm -rf ./bin ./dist ./typescript/bin",
    "prepublish": "npm run clean && tsc -p ./typescript/tsconfig.json",
    "generate": "ts-node ./typescript/script/generate.ts ./example/typescript/index.ts ./example/tsconfig.json",
    "test": "TS_NODE_PROJECT=./typescript/tsconfig.json mocha --compilers ts:ts-node/register ./typescript/test/**/*.ts"
  },
  "author": "Derek Honerlaw",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.0.25",
    "@types/yamljs": "^0.2.30",
    "chai": "^4.1.1",
    "inversify": "^4.3.0",
    "mocha": "^3.5.0",
    "reflect-metadata": "^0.1.10",
    "ts-node": "^3.3.0",
    "tslint": "^5.6.0",
    "typemoq": "^2.0.1",
    "typescript": "^2.4.2",
    "yamljs": "^0.3.0"
  }
}
